class Switch1
{
	public static void main(String[] args)
	{
		int ip = 1;
		switch(ip)
		{
			case 1:
			{
				System.out.println("From case 1"); //Executed
				break;
			}
			case 2:
			{
				System.out.println("From case 2");
				break;
			}
			case 3:
			{
				System.out.println("From case 3");
				break;
			}
		}
	}
}