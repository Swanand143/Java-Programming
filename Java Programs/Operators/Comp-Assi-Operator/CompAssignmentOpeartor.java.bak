class CompAssignmentOpeartor
{
	public static void main(String[] args)
	{
		int sal = 5000;
		int sal+=5000;
		System.out.println("+= : "+sal);

		int sal1 = 10000;
		int sal1 -= 5000;
		System.out.println("-= : "+sal1);

		int sal2 = 25000;
		int sal2 *= 2;
		System.out.println("*= : "+sal2);

		int sal3 = 20000;
		int sal3 /= 5;
		System.out.println("/= : "+sal3);

		int sal4 = 1000;
		int sal4 %=13;
		System.out.println("%= : "+sal4);
	}
}