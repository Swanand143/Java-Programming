import java.util.Scanner;
class CheckLowerCase
{
	public static void main(String [] args)
	{
	Scanner sc = new Scanner(System.in);
	System.out.print("Enter a character: ");
	char ch = sc.next().charAt(0);

	String charValue = (ch>='a'&&ch<='z')?(ch+" is a LowerCase Alphabet"):(ch+" is not a LowerCase Alphabet");

	//String charValue = ((ch>='A'&& ch<='Z')||(ch>='a'&& ch<='z'))?
	//((ch>='a'&& ch<='z')?(ch+" is a LowerCase Alphabet"):(ch+" it is an UpperCase Alphabet")):
		//((ch>='0'&&ch<'9')?(ch+" it is Digit"):(ch+" it is an Special Character"));
	System.out.println(charValue);
	}
}



