import java.util.Scanner;
class PrimeNumber
{
	public static void main(String [] args)
	{
		System.out.print("Enter a number: ");
		int num = new Scanner(System.in).nextInt();
	    System.out.println((num==2||num==3||num==5)?(num+" is prime no."):((num%2==0||num%3==0||num%5==0)?(num+ " is not prime no."):(num+" is prime no.")));
	}


}