class Addition
{
	public static void main(String[] args)
	{
		byte opr1 = 20;
	    byte opr2 = 30;
		int op1 = opr1 + opr2;
		System.out.println(op1); // byte + byte = int 

		short opr3 = 200;
	    short opr4 = 300;
		int op2 = opr3 + opr4;
		System.out.println(op2); // short + short = int

		char opr5 = 'A';
	    char opr6 = 'B';
		int op3 = opr5 + opr6;
		System.out.println(op3); // char + char = int

		int opr7 = 2000;
	    int opr8 = 3000;
		int op4 = opr7 + opr8;
		System.out.println(op4); // int + int = int 

		long opr9 = 250l;
	    long opr10 = 5000l;
		long op5 = opr9 + opr10;
		System.out.println(op5); // long + long = long

		float opr11 = 1.1f;
	    float opr12 = 2.2f;
		float op6 = opr11 + opr12;
		System.out.println(op6); // float + float = float
		
		double opr13 = 1.1;
	    double opr14 = 2.2;
		double op7 = opr13 + opr14;
		System.out.println(op7); // double + double = double

		String opr15 = "Java";
		String opr16 = "Program";
		String op8 = opr15 + opr16;
		System.out.println(op8); // String + String = String

		//boolean opr17 = true ;
		//boolean opr18 = false;
		//boolean op9 = opr17 + opr18;
		//System.out.println(op9); // CTE
	}
}