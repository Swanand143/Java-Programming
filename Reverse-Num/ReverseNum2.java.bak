import java.util.Scanner;
class ReverseNum2
{
	public static void main(String[] args)
	{
		Scanner ab = new Scanner(System.in);
		System.out.print("Enter a number : ");
		int num = ab.nextInt();
		int rem = 0;
		int rev = 0;
		while (num>0)
		{
			rem = num%10;
			rev = (rev*10)+rem;
			num/=10;
		}
		System.out.println("Reverse No : "rev);
	}
}