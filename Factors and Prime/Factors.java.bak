import java.util.Scanner;
class Factors
{
	public static void main(String[] args)
	{
	Scanner ab = new Scanner(System.in);
	System.out.print("Enter the number : ");
	int num = ab.nextInt();
	int cnt = 0;
	int sum = 0;
	for (int i=1; i<=num; i++ )
	{
		if (num%i==0)	
		{
			cnt++;
			sum += i
			System.out.print(i+" ");
		}
		
	}
	System.out.println();
    System.out.print("Total factors are : "+cnt);
	}
}