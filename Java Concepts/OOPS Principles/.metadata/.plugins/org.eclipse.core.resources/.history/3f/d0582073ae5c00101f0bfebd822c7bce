package arraysprogram;

public class FreqOfElement {
	
	public static void main(String[] args) {
		int[] arr = {1,1,1,2,2,3,4}; 
		System.out.println(frqOfElement(arr));
	}
	
	public static void frqOfElement(int[] arr)
	{
		int cnt = 0;
		int cnt2 = 0;
		for (int i = 0; i < arr.length; i++) {
			cnt = 0;
			if (arr[i] != cnt2) {
				cnt2 = 0;
				for (int j = 0; j < arr.length; j++) {
					if (arr[i]==arr[j]) {
						cnt++;
						cnt2 = arr[i]; 
					}
				}
				System.out.println(arr[i]+" : "+cnt);
			}
			
		}
	}
	

}
